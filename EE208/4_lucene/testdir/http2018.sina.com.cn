            againstUrl: '//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=livecast&_sport_a_=matchesbytype&type=108',
            rndArr: [4,5,6,7,8],
            scorerUrl: '//2018.sina.com.cn/api/static/player_rank.js',
            itemArr: [13, 2, 6], //进球13，助攻2，扑救6
            teamUrl: '', //   '//2014.sina.com.cn/teams/colombia.html?team_id=',
            playerUrl: '//2018.sina.com.cn/ballgame/player.shtml?id=',
            uuid: '-1da9fd2f-5595-4563-ab46-0570c721f313'.replace(/-/g, '_')
        };
        /*积分榜*/
        if($('#scoreboard').length){
            $.ajax({
                url: SCOREBOARD.scoreboardUrl,
                cache: false,
                jsonpCallback: 'cb_team_order',
                type: 'GET',
                dataType: 'jsonp',
                success: function (res) {
                    res = res.result;
                    scoreHandle(res);
                },
                fail: function (error) {
                    scoreHandle(error);
                    console.warn(error);
                }
            });
            function scoreHandle(res) {
                var templs = function (d, e, f) {
                    return [
                        f(d, function (l, key) {
                            return [
                                '<div class="score-cont', (key == 'A' ? ' cont-selected' : ''), '" style="display:none;" tab-type="tab-cont">',
                                '<table class="score-cont_table">',
                                '<thead>',
                                '<tr>',
                                '<th>排名</th>',
                                '<th>国家</th>',
                                '<th>场次</th>',
                                '<th>积分</th>',
                                '</tr>',
                                '</thead>',
                                '<tbody>',
									(!l ||　l.length == 0 ? '<tr><td colspan="4">暂无数据</td></tr>' :
                                        f(l, function (item, index) {
                                            return [
                                                '<tr>',
                                                '<td>', item.team_order, '</td>',
                                                '<td><a class="scoreboard_name" target="_blank" href="', (TEARMURL32&&TEARMURL32[item.sl_id] ? (TEARMURL32[item.sl_id].url) : 'javascript:;'), '">', item.team_cn, '</a></td>',
                                                '<td>', item.count, '</td>',
                                                '<td>', item.score, '</td>',
                                                '</tr>'
                                            ]
                                        }) ),
                                '</tbody>',
                                '</table>',
                                '</div>'
                            ]
                        })
                    ]
                };
                var templData = {'A': [], 'B': [],'C': [],'D': [],'E': [],'F': [],'G': [],'H': []};
                if (res.status && res.status.code === 0 && res.data && res.data.length != 0) { templData = $.extend(true, templData, res.data); }
                $('#scoreboard').append( Render(templData, templs));
            }
        }
        /*对阵图*/
        if($('#against').length){
            $.ajax({
                url: SCOREBOARD.againstUrl,
                data: {
                    fields: 'group,status,Team1,Team2,Team1Id,Team2Id,Score1,Score2,Round',
                    limit: 64,
                    season: SCOREBOARD.season,
                    dpc: 1
                },
                type: 'GET',
                dataType: 'jsonp',
                cache: true,
                jsonpCallback: 'cb_against' + SCOREBOARD.uuid,
                success: function (res) {
                    res = res.result;
                    againstHandle(res);
                },
                fail: function (error) {
                    againstHandle(error);
                    console.warn(error);
                }
            });
            function againstHandle(res) {
                var templs = function (d, e, f) {

                    return [
                        (!d || d.length == 0 ? '' :
                                        f(d, function (item, index) {
                                            return [
                                                '<tr>',
                                                '<td class="team1"><a class="" target="_blank" href="', (TEARMURL32&&TEARMURL32[item.Team1Id] ? (TEARMURL32[item.Team1Id].url) : 'javascript:;'), '">', item.Team1, '</a></td>',
                                                '<td class="team-score"',item.Round==7||item.Round==8?' style="width:74px;"':'','>', (item.status == '3' ? item.Score1 + '-' + item.Score2 : 'VS'),item.Round==7?'(三四名)':'','</td>',
                                                '<td class="team2"><a class="" target="_blank" href="', (TEARMURL32&&TEARMURL32[item.Team2Id] ? (TEARMURL32[item.Team2Id].url) : 'javascript:;'), '">', item.Team2, '</a></td>',
                                                '</tr>'
                                            ]
                                        })
                        )
                    ]
                };
                var templsData = [],
                        rndArr = SCOREBOARD.rndArr,
                        rndIndex, dataIndex,
                        domArr = $("#against .against-data");
                for (rndIndex = 0; rndIndex < rndArr.length; rndIndex++) {
                    var rndCur = rndArr[rndIndex];
                    templsData[rndIndex] = [];
                    if (res.status && res.status.code === 0 && res.data && res.data.length != 0) {
                        for(dataIndex = 0; dataIndex < res.data.length; dataIndex++) {
                            var dataCur = res.data[dataIndex];
                            if (dataCur.Round - rndCur == 0){ templsData[rndIndex].push(dataCur); }
                        }
                    }
                    domArr.eq(rndIndex).html( Render(templsData[rndIndex], templs));
                }
                var nowTime = new Date().getTime();
                var nowIndex = 0;
                if(nowTime > new Date('2018/07/12 04:00:00').getTime()){
                    nowIndex = 3;
                }else if(nowTime > new Date('2018/07/08 04:00:00').getTime()){
                    nowIndex = 2;
                }else  if(nowTime > new Date('2018/07/04 04:00:00').getTime()){
                    nowIndex = 1;
                }
                $('#against .score-nav').eq(nowIndex).addClass('nav-selected').siblings().removeClass('nav-selected');
                $('#against .score-cont').eq(nowIndex).addClass('cont-selected').siblings().removeClass('cont-selected')
            }
        }
        /*射手榜*/
        if($('#scorer').length){
            $.ajax({
                url: SCOREBOARD.scorerUrl,
                cache: false,
                jsonpCallback: 'cb_player_order',
                type: 'GET',
                dataType: 'jsonp',
                success: function (res) {
                    res = res.result;
                    playerHandle(res);
                },
                fail: function (error) {
                    playerHandle(error);
                    console.warn(error);
                }
            });
            function playerHandle(res) {
                var templs = function (d, e, f) {
                    return [
                        (!d || d.length == 0 ? '<tr><td colspan="3">暂无数据</td></tr>' :
                                        f(d, function (item, index) {
                                            return [
                                                '<tr>',
                                                '<td>', item.num, '</td>',
                                                '<td><a class="scoreboard_name" target="_blank" href="', (SCOREBOARD.playerUrl ? (SCOREBOARD.playerUrl + item.player_id) : 'javascript:;'), '">', item.player_name, '</a></td>',
                                                '<td>', item.item1, '</td>',
                                                '</tr>'
                                            ]
                                        })
                        )
                    ]
                };
                var templsData = '', itemIndex;
                // 进球13，助攻2，扑救6
                var domCon = $('#scorer .palyer-data');
                for (itemIndex = 0; itemIndex < SCOREBOARD.itemArr.length; itemIndex++) {
                    var item = SCOREBOARD.itemArr[itemIndex];
                    if (res.status && res.status.code === 0 && res.data && res.data[item] && res.data[item].length != 0) { templsData = res.data[item]; }
                    domCon.eq(itemIndex).html( Render(templsData, templs)  );
                }
            }
        }
    });

</script>
