        <script>
            requirejs(['sportsTianyi3'],function(sportsTianyi3){
                var mainFeedTab=$('#ty__title-tabs');
                var eleTop = mainFeedTab.offset().top;
                var bind = function(){
                    $(window).on('scroll',function(){
                        var top=$(window).scrollTop();
                        if(top>eleTop){
                            mainFeedTab.addClass('ty__title-fixed');
                        }else{
                            mainFeedTab.removeClass('ty__title-fixed');
                        }
                    })
                }
                bind();
                var smoothScroll=function(wrap,height){
                    var gap = parseInt(wrap.offset().top);
                    var top = $(window).scrollTop();
                    if(top > gap){
                        $('body,html').scrollTop(gap);
                    }
                }

                var tabWrap=$('#ty__title-tabs');
                var contWrap=$('#j_tianYiMainFeed');
                var tyTop = $('#ty-top-ent0');

                tabWrap.on('click','.mainfeed__item', function(){
                    var $this=$(this);
                    // 切换置顶
                    if ($this.attr('tab-item') === 'FeedTab_tab0') {
                        tyTop.show(0);
                    } else {
                        tyTop.hide(0);
                    }
                    var sudaclick=$this.attr('data');
                    contWrap.attr('data-sudaclick',sudaclick);
                    $('.mainfeed__item-cur').removeClass('mainfeed__item-cur');
                    $this.addClass('mainfeed__item-cur');
                    smoothScroll(contWrap,60);
                    var mainFeedItem=$this.attr('tab-item');
                    sportsTianyi3.changeList(mainFeedItem);
                });
            })
        </script>
