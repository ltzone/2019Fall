//图片上传后回调函数
function pic_upload_callback(json){
	if(json.error == 1)
	{
		$("#pic_"+json.pic_id).html("");
		$("#pic_"+json.pic_id).hide();
		show_tips(json.pic_id, json.errmsg);
		window.$.tools.AdjustCovers();
	}else {
		$("#tip_"+json.pic_id).html("");
		$("#pic_"+json.pic_id).show();
		$("#pic_"+json.pic_id).html("<img src='"+json.pic_large+"' onload='window.$.tools.AdjustCovers();' style='max-width:200px;border:2px solid #b8b8b8;'/>");
		$("#"+json.pic_id).val(json.pic_large);
	} 
};

//在表单项显示提示
function show_tips(id,msg)
{
	$("#tip_"+id).html('<i class="icon-hill">&nbsp;</i>'+msg).show();
	$(window).scrollTop($("#tip_"+id).offset().top);
};

//显示加载
function show_loading(id){
	$("#pic_"+id).html("<img src='/images/ajax.gif' width='32px' height='32px'/>").show();
};

//获取城市
function get_city(id,val)
{
	$.get('/interface/common/province.php?province_id='+val, 
		function(json){
			if(json.error==1){
				alert("没有这个省。。");
			}else{
				$("#city_"+id).html('');
				$.each(json.city_list, function (index, value) {
					$("#city_"+id).append("<option value='"+value.id+"'>"+value.name+"</option>");
                });
			}
		},
		'jsonp');
};

//提交表单
function form_submit()
{
	$(".error-tip").html("").hide();
	$.post("/interface/activity/json_add_signup.php?act_id=2349",
		$("#form_div").serialize(),
		function(data)
		{
			if (data.error == 1)
			{
				try{
					var error_arr = $.parseJSON(data.errmsg);
					$.each(error_arr,function(i,n){
						show_tips(n[0],n[1]);
					});
				}catch(e){
					InfoDlg(data.errmsg);
				}
			}
			else
			{
				if(typeof(custom_func) == "undefined"){
					custom_func = function(){};
				}
				show_draw_layer(data, '提交成功', function(){
					InfoDlg('提交成功', 3000, custom_func);
				},custom_func);

				//不刷新页面（由于中奖弹层没有回调所以不能刷新），重置表单项
				$("#form_div").get(0).reset();
				$("#form_div [type='hidden']").val("");//chrome/firefox下，reset无法重置hidden表单项
				$("#form_div .show-pic").hide();
			}
		}
		,'jsonp');
};
