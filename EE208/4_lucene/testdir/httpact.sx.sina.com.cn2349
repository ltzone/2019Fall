<script type="text/javascript">
/**
 * 显示抽奖结果层，如果没开启抽奖，则调用func
 * @param  {[type]} json  [description]
 * @param  {[type]} title [description]
 * @param  {[type]} func  [如果没有抽奖则调用的函数]
 * @param  {[type]} draw_func  [抽奖流程完毕后调用的函数]
 * @return {[type]}       [description]
 */
function show_draw_layer(json, title, func, draw_func){
	if(!func){
		func = function(){ };
	}
	if(!draw_func){
		draw_func = function(){ };
	}
	if(!title){
		title = '投票成功，点击确定碰碰运气吧！';
	}

	var div = $('div#dialog_prize_div');

	if(json.draw && json.draw.tip && json.draw.tip.text) {
		InfoDlg(title, '', function() {
			$('h3', div).text((json.draw.tip.text));
			//奖品图片显示
			if(json.draw.tip.image) {
				$('.prize-pic img', div).attr('src',(json.draw.tip.image));
			} else {
				$('.prize-pic', div).hide();
			}
			//分享至微博控制
			if(!$.isEmptyObject(json.draw.tip.weibo_msg)){
				$('.send_to_weibo', div).show();
			}else{
				$('.send_to_weibo', div).hide();
			}

			var checkbox = null;//指向弹出层的checkbox
			var layer = InfoDlg(div.find('div.prize-div').html(), '', function(){
				//如果选中了发微博复选框，则发微博
				if(checkbox.attr("checked") && !$.isEmptyObject(json.draw.tip.weibo_msg)){
					$.post('/interface/activity/json_add_mblog.php', 
						{'content':json.draw.tip.weibo_msg.text, 'pic_url':json.draw.tip.weibo_msg.image, 'act_id':$ACT_INFO.ID}, 
						function(json){
							draw_func();
						}
					);
				}else{
					draw_func();
				}
			})._$node;
			checkbox = $(':checkbox[name="send_to_weibo"]', layer);
			layer.find('.F_btn').css({textAlign:'center'}).end().find('.layer_point .point dd').css({width:'auto'});//弹出层的样式调整
		});
	} else {
		func();
	}
}
</script><script type="text/javascript">
