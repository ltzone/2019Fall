function getCfStr() {
    var ts = parseInt(new Date / 1000, 10);
    return "&_ts=" + ts
        + "&_token=" + bds.cf.token 
        + "&_sign=" + md5(ts +  bds.cf.token + '3b6e2e87fcce6eea8337eeac83b478f5');
}

function getCfObj() {
    var ts = parseInt(new Date / 1000, 10);
    return {
            '_ts' : ts,
            '_token' : bds.cf.token,
