                            $(window).on('load', function(e){
                if(!$.browser.ie || $.browser.ie > 7){
                    $('.ikqb_img_alink').each(function(i, item){
                        var img = $(item).find('.ikqb_img').get(0);
                        var imgWidth = $(img).width(),
                            imgHeight = $(img).height(),
                            conOffX = $(item).parent().offset().left,
                            imgOffX = $(img).offset().left,
                            imgDiff = imgOffX - conOffX,
                            imgWidthMax = 600 - imgDiff,
                            tmpImgHeight = imgHeight > imgWidthMax ? imgWidthMax : imgHeight,
                            tmpImgWidth = imgHeight > imgWidthMax ? (imgWidthMax*imgWidth/imgHeight) : imgWidth,
                            imgOff = (tmpImgWidth - tmpImgHeight)/2;
                        var html = [
                            '<div class="img-turn f-12">',
                            '<span class="img-turn-left"><i class="i-turn-zuo mr-5"></i>向左转</span>',
                            '<span class="f-pipe">|</span>',
                            '<span class="img-turn-right"><i class="i-turn-you mr-5"></i>向右转</span>',
                            '</div>'
                        ].join('');
                        $(item).before(html);
                        $(item).prev().css({
                            'left': imgDiff+'px'
                        });
                        // 此处是因为图片居中样式
                        imgDiff = 0
                        $(img).css({
                            'position': 'relative',
                            'top': 0,
                            'left': imgDiff+'px'
                        }).addClass('css-turn-0');
                        $(item).prev().find('.img-turn-left').click(function(e){
                            log.send({
                                type: 2014,
                                action: 'click',
                                page: 'question',
                                area: 'img-turn-left'
                            });
                            var hasFind = false;
                            for(var i = 0; i < 360; i = i + 90){
                                if(hasFind){
                                    return;
                                }
                                if($(img).hasClass('css-turn-'+i)){
                                    hasFind = true;
                                    var index = i == 0 ? 360 : i;
                                    $(img).removeClass('css-turn-'+i)
                                        .addClass('css-turn-'+(index-90));
                                    if(index-90 == 90 || index-90 == 270){
                                        if(!$.browser.ie || $.browser.ie > 9){
                                            $(img).css({
                                                'position': 'relative',
                                                'top': imgOff+'px',
                                                'left': -(imgOff - imgDiff)+'px'
                                            });
                                        }
                                        if(imgHeight > imgWidthMax){
                                            $(img).height(tmpImgHeight);
                                        }
                                        $(item).height(tmpImgWidth);
                                    }else {
                                        $(img).css({
                                            'position': 'relative',
                                            'top': 0,
                                            'left': imgDiff+'px'
                                        });
                                        if(imgHeight > imgWidthMax){
                                            $(img).height(imgHeight);
                                        }
                                        $(item).height(imgHeight);
                                    }
                                }
                            }
                        });
                        $(item).prev().find('.img-turn-right').click(function(e){
                            log.send({
                                type: 2014,
                                action: 'click',
                                page: 'question',
                                area: 'img-turn-right'
                            });
                            var hasFind = false;
                            for(var i = 0; i < 360; i = i + 90){
                                if(hasFind){
                                    return;
                                }
                                if($(img).hasClass('css-turn-'+i)){
                                    hasFind = true;
                                    var index = i == 270 ? (-90) : i;
                                    $(img).removeClass('css-turn-'+i)
                                        .addClass('css-turn-'+(index+90));
                                    if(index+90 == 90 || index+90 == 270){
                                        if(!$.browser.ie || $.browser.ie > 9){
                                            $(img).css({
                                                'position': 'relative',
                                                'top': imgOff+'px',
                                                'left': -(imgOff - imgDiff)+'px'
                                            });
                                        }
                                        if(imgHeight > imgWidthMax){
                                            $(img).height(tmpImgHeight);
                                        }
                                        $(item).height(tmpImgWidth);
                                    }else {
                                        $(img).css({
                                            'position': 'relative',
                                            'top': 0,
                                            'left': imgDiff+'px'
                                        });
                                        if(imgHeight > imgWidthMax){
                                            $(img).height(imgHeight);
                                        }
                                        $(item).height(imgHeight);
                                    }
                                }
                            }
                        });
                    });
                }
            });
        
    $(window).load(function () {
        if (F.context('pageType') === 'asker' && !Number(F.context('page')['delAsk'])) {
            sendMore.default.init();
        }
        ec.fire('pageLoaded');
