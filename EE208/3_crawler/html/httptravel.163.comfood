function getElementPos(el) {
	var ua = navigator.userAgent.toLowerCase();
	var isOpera = (ua.indexOf('opera') != -1);
	var isIE = (ua.indexOf('msie') != -1 && !isOpera); // not opera spoof 
	if (el.parentNode === null || el.style.display == 'none') {
		return false;
	}
	var parent = null;
	var pos = [];
	var box;
	if (el.getBoundingClientRect) { //IE
		box = el.getBoundingClientRect();
		var scrollTop = Math.max(document.documentElement.scrollTop, document.body.scrollTop);
		var scrollLeft = Math.max(document.documentElement.scrollLeft, document.body.scrollLeft);
		return {
			x: box.left + scrollLeft,
			y: box.top + scrollTop
		};
	} else if (document.getBoxObjectFor) { // gecko
		box = document.getBoxObjectFor(el);
		var borderLeft = (el.style.borderLeftWidth) ? parseInt(el.style.borderLeftWidth) : 0;
		var borderTop = (el.style.borderTopWidth) ? parseInt(el.style.borderTopWidth) : 0;
		pos = [box.x - borderLeft, box.y - borderTop];
	} else { // safari & opera 
		pos = [el.offsetLeft, el.offsetTop];
		parent = el.offsetParent;
		if (parent != el) {
			while (parent) {
				pos[0] += parent.offsetLeft;
				pos[1] += parent.offsetTop;
				parent = parent.offsetParent;
			}
		}
		if (ua.indexOf('opera') != -1 || (ua.indexOf('safari') != -1 && el.style.position == 'absolute')) {
			pos[0] -= document.body.offsetLeft;
			pos[1] -= document.body.offsetTop;
		}
	}
	if (el.parentNode) {
		parent = el.parentNode;
	} else {
		parent = null;
	}
	while (parent && parent.tagName != 'BODY' && parent.tagName != 'HTML') { // account for any scrolled ancestors 
		pos[0] -= parent.scrollLeft;
		pos[1] -= parent.scrollTop;
		if (parent.parentNode) {
			parent = parent.parentNode;
		} else {
			parent = null;
		}
	}
	return {
		x: pos[0],
		y: pos[1]
	};
}

function bindShowShareList(){
	var NTESSelectMenu = function() {
		var t = this;
		t.selector = '.js-ep-pop';
		t.unfoldClassName = 'ep-pop-unfold';
		t.menus = NTES(t.selector);
		t.init();
		t.prevIndex = 0;
	}
	NTESSelectMenu.prototype = {
		constructor : NTESSelectMenu,
		init : function() {
			var t = this;
			NTES.each(t.menus, function(i, item){
		
				var _item = NTES(item);
				
				// 鼠标进入下拉菜单区域内时，菜单为展开状态，鼠标移出后收起
				// 通过延时避免瞬间操作触发菜单展开/收起事件（IE下，在标题和下拉菜单区域间移动时，会瞬间触发收起）
				new NTES.event.Mouseenter('mouseenter', function(ev){
					var target = NTES(this);
					if (t.timerList && t.prevIndex === i) {
						clearTimeout(t.timerList);
						t.timerList = undefined;
					} else {
						t.timerList = setTimeout(function(){
							t.unfoldMenu(i);
							t.timerList = undefined;
						}, 100);
					}
				}, _item);
				new NTES.event.Mouseenter('mouseleave', function(ev){
					var target = NTES(this);
					if (t.timerList && t.prevIndex === i) {
						clearTimeout(t.timerList);
						t.timerList = undefined;
					} else {
						t.timerList = setTimeout(function(){
							t.foldMenu();
							t.timerList = undefined;
						}, 100);
					}
				}, _item);
		
				//点击可控制展开/收起（不阻止链接，如果是链接的话优先打开链接）
				//键盘控制，右键/下键弹出菜单，左键/上键/esc收起菜单，tab键导航
				_item.addEvent('click', function(ev){
					var target = NTES(this);
					target.className.indexOf(t.unfoldClassName) === -1 ? target.addCss(t.unfoldClassName) : target.removeCss(t.unfoldClassName);
				}).addEvent('keydown', function(ev){
					var target = NTES(this);
					switch (ev.keyCode) {
						case 39 : t.unfoldMenu(i); break;
						case 40 : t.unfoldMenu(i); break;
						case 37 : t.foldMenu(); break;
						case 38 : t.foldMenu(); break;
						case 27 : t.foldMenu(); break;
						default : break;
					}
				});
			});
		},
		unfoldMenu : function(i) {
			var t = this;
			NTES(t.menus[t.prevIndex]).removeCss(t.unfoldClassName);
			NTES(t.menus[i]).addCss(t.unfoldClassName);
			t.prevIndex = i;
			
		},
		foldMenu : function() {
			var t = this;
			NTES(t.menus[t.prevIndex]).removeCss(t.unfoldClassName);
		}
	}
	new NTESSelectMenu();
}
function addColor(sClass){
	var aLi = NE('.'+sClass+' li');
	for(var i=0; i<aLi.length; ++i){
		aLi[i].index = i;
		aLi[i].onmouseover = function(){
			for(var i=0;i<aLi.length; ++i){
				aLi[i].style.zIndex = 20;
				aLi[i].style.backgroundColor = '#fff';
			}
			this.style.backgroundColor = '#fafafa';
			this.style.zIndex = 21;
		}
		aLi[i].onmouseout = function(){
			this.style.zIndex = 20;
		}
	}
}
function showFoodPage(tag, pageno) {
