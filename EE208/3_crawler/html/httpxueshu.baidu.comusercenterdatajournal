    var c = G('container'),w = G('wrapper'),
    remove = function(r,o){o.className = o.className.replace(r, '');},

    add = function(cls,o){o.className = (o.className+' '+cls).replace(/^\s+/g, "");},
    has = function(r,o){return r.test(o.className)};
