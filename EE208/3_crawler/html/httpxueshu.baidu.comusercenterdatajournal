/**
* @description 增加xueshu nsclick统计
* @function
* @name c
* @grammar ns_c(1)
* @param {Object} q 字段集合
* @return {Boolean} 当发送成功时返回true
*/
function ns_c_xs(q){
    var p = encodeURIComponent(window.document.location.href), sQ = '',wd="", sV = '', mu='', img = window["BD_PS_C" + (new Date()).getTime()] = new Image();
    for (v in q) {
        switch (v) {
            case "title":
                sV = encodeURIComponent(q[v].replace(/<[^<>]+>/g, ""));
                break;
            case "url":
                sV = encodeURIComponent(q[v]);
                break;
            default:
                sV = q[v]
        }
        sQ += v + "=" + sV + "&";
    }
    mu= "&mu=" + p;
    img.src = "http://nsclick.baidu.com/v.gif?pid=201&pj=mp_xueshu&qid=" + bds.comm.qid + "&" + sQ + "f=&fr=journal_page&path=" + p + "&tn=ae_xueshu_journal&t=" + new Date().getTime();
    return true;
}
