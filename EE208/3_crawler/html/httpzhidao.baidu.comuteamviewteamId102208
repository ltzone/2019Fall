        <div class="ask"><span class="green">问：</span>${listItem.qtitle}</div>
        <div class="ans"><span class="orange">答：</span>${listItem.rContent}</div>
        {@if listItem.fType==2}
            {@if listItem.rAuthentic==undefined || listItem.rAuthentic==0}
                {@if listItem.isGood==3}<span class="pa q-ico q-adopt2 q-adopt3"></span><span class="pa q-ico q-adopt1"></span>{@else}<span class="pa q-ico q-adopt${listItem.isGood}"></span>{@/if}
            {@else}
                {@if listItem.rAuthentic==2}<span class="pa ico-auth ico-auth-2">专业回答</span>{@else}<span class="pa ico-auth ico-auth-1">认证中</span>{@/if}
            {@/if}
        {@/if}
    </li>
    {@else}
    <li class="sp-list">
        <span class="pa time">${listItem.time}</span><em class="pa{@if listIndex==0} first-em{@/if}"></em>
