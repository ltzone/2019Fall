<script>
    document.domain = "163.com";
        var INIT_ADS_DATA = {
        items: [
                                                        {
                        url: "https://open.163.com/special/knowledge_h5/?p=tonglan-zssd",
                        lspri: "80",
                        imgSrc: "http://cms-bucket.ws.126.net/2019/06/10/695b137373b740f7b29d2f997b194f21.jpeg",
                        adsType: "H5_TONGLAN"
                    },
                                        {
                        url: "https://open.163.com/special/knowledge_pc/?p=duilian-zssd",
                        lspri: "80",
                        imgSrc: "http://cms-bucket.ws.126.net/2019/06/10/9548ce141b3f4b07a079f619057587fc.jpeg",
                        adsType: "PC_DUILIAN"
                    },
                                        {
                        url: "https://open.163.com/special/knowledge_pc/?p=tonglan-zssd",
                        lspri: "80",
                        imgSrc: "http://cms-bucket.ws.126.net/2019/06/10/20a93ac55ccf4c5687384a3640f08e16.jpeg",
                        adsType: "PC_TONGLAN"
                    },
                                                ]
        };
    ; (function () {
        var OPEN_ADS_DATA = {
            pc: {
                tonglan:{},
                duilian:{}
            },
            h5: {
                tonglan:{}
            }
        };
        for (var i = 0, len = INIT_ADS_DATA.items.length, ads; i < len; i++) {
            ads = INIT_ADS_DATA.items[i]
            if (ads.adsType === "PC_TONGLAN") {
                OPEN_ADS_DATA.pc.tonglan = ads;
            } else if (ads.adsType === "PC_DUILIAN") {
                OPEN_ADS_DATA.pc.duilian = ads;
            } else if (ads.adsType === "H5_TONGLAN") {
                OPEN_ADS_DATA.h5.tonglan = ads;
            }
            delete ads.adsType;
        }
        top.OPEN_ADS_DATA = window.OPEN_ADS_DATA = OPEN_ADS_DATA;
    })();
</script>
