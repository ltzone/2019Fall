<script>
  $('#app').on('focus','.j-txt',function(){
		$(this).parent().addClass('active');
		$(this).parent().find('label').hide();
	}).on('blur','.j-txt',function(){
		$(this).parent().removeClass('active');
		if (!$(this).val()) {
			$(this).parent().find('label').show();
