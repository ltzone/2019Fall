    function showDiscountAd() {
        var discountAdDom = document.getElementById('discount-ad');
        try {
            if (parseFloat(OPEN_ADS_DATA.pc.duilian.lspri) >= 60 && document.body.clientWidth > 1600) {
              
                discountAdDom.innerHTML = 
                '<a href="' + OPEN_ADS_DATA.pc.duilian.url + '">\
                    <img class="s-ad s-left-ad" src="'+ OPEN_ADS_DATA.pc.duilian.imgSrc + '" alt="">\
                </a>\
                <a href="'+ OPEN_ADS_DATA.pc.duilian.url + '">\
                    <img class="s-ad s-right-ad" src="'+ OPEN_ADS_DATA.pc.duilian.imgSrc + '" alt="">\
                </a>';
           
                discountAdDom.style.display = 'block';
              
            } else {
              
                discountAdDom.style.display = 'none';
            }
        } catch (err) { }
    }
  setTimeout(function(){
  //showDiscountAd();
},1000)
    
