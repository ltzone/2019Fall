        var $container = $('#j-question-list-pjax-container');

        $container.on('click', '.title-link', function (e) {
            e.preventDefault();
            var dedData = {};
            var $this = $(e.currentTarget);
            var href = $this.attr('href');
            if ($.url.getQueryValue(location.href, 'fr') === 'datichoujiang') {
                // 新用户答题抽奖引导统计
                href += '&entry=datichoujiang';
            } else if (!$.trim($('.list-section-widget-wp').find('.search-text').val())) {
                dedData = ded.getLogData();
                href += '&entry=list_' + dedData.nav +'_' + dedData.classify ;
            } else {
                dedData = ded.getLogData();
                dedData['classify'] = 'search';
                href += '&entry=list_' + dedData.nav +'_search';
            }
            // isFresh|isThreePoint字段统计
            var isFresh = $this.parents('.question-list-item').data('isfresh') || 0;
            var qid = $this.parents('.question-list-item').data('qid') || 0;
            var isThreePoints = $this.parents('.question-list-item').data('isthreepoints') || 0;
            dedData['isfresh'] = isFresh;
            dedData['qid'] = qid;
            dedData['isthreepoints'] = isThreePoints;
            window.open(href);
            // 统计答题用户中新老用户的比例
            log.send($.extend({
                page: 'list',
                area: 'title_link',
                action: 'click',
                answer_num: '0',
            }, dedData));
        });
    });
