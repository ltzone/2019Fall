				var G_USER = (function getLoginInfo(){
				var e = {
		          isLogin: !1,
		          userId: null
		      },
		      t=$.cookie("P_INFO"),
		      n=$.cookie("S_INFO");
		      if(t){
		        t=t.split("|"),
		        e.userId=t[0];
		        if(t[2]==1||t[2]==0&&n)
		          e.isLogin=!0
		      }
		      return e;
		    })();
		    </script>

